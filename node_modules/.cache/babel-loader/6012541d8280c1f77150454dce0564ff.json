;AAAA;AAAA;AAAA;AAAA,IADF,CADF,EAIE;AAAK,IAAA,KAAK,EAAC,cAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,KAAK,EAAC,eAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAI,IAAA,KAAK,EAAC,+BAAV;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,UADF,8BADF,CADF,EAME;AAAK,IAAA,KAAK,EAAC,aAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAI,IAAA,KAAK,EAAC,iCAAV;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ogBADF,EAME;AAAG,IAAA,KAAK,EAAC,6BAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,0zCANF,CADF,EAeE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAI,IAAA,KAAK,EAAC,iCAAV;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qTADF,EAIE;AAAG,IAAA,KAAK,EAAC,6BAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,0zCAJF,CAfF,EA2BE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAI,IAAA,KAAK,EAAC,iCAAV;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,8ZADF,EAKE;AAAG,IAAA,KAAK,EAAC,6BAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,s2CALF,CA3BF,CANF,CAJF,CADF,CADF;AAyDH,CA1DD;;AA4DA,eAAeC,KAAf","sourcesContent":["import React, { Component } from \"react\";\nimport \"./about.scss\";\nimport \"./pagination.scss\";\nimport about from '../../images/group.jpg';\n\nconst About = () => {\n    return (\n      <section class=\"section\">\n        <div class=\"about\">\n          <div class=\"about__image\">\n            <img src={about} alt=\"Uma.Tech\" />\n          </div>\n          <div class=\"about__right\">\n            <div class=\"about__header\">\n              <h2 class=\"typography typography--header\">\n                <span>| </span>О нас\n              </h2>\n            </div>\n            <div class=\"about__text\">\n              <div>\n                <h3 class=\"typography typography--headersm\">\n                  Мы&nbsp;&mdash; высокотехнологичная компания\n                  с&nbsp;многолетним опытом работы на&nbsp;рынке&nbsp;ИТ\n                  и&nbsp;телекоммуникаций.\n                </h3>\n                <p class=\"typography typography--text\">\n                  Создавая комплексные решения для медиа-индустрии, мы построили\n                  одну из крупнейших федеральных сетей доставки видеоконтента,\n                  ряд уникальных технических решений и отработали гибкие\n                  бизнес-схемы, что позволяет нам удовлетворять запросы любого\n                  заказчика.\n                </p>\n              </div>\n              <div>\n                <h3 class=\"typography typography--headersm\">\n                  Наш опыт прекрасно отвечает запросам современного рынка.\n                </h3>\n                <p class=\"typography typography--text\">\n                  Создавая комплексные решения для медиа-индустрии, мы построили\n                  одну из крупнейших федеральных сетей доставки видеоконтента,\n                  ряд уникальных технических решений и отработали гибкие\n                  бизнес-схемы, что позволяет нам удовлетворять запросы любого\n                  заказчика.\n                </p>\n              </div>\n              <div>\n                <h3 class=\"typography typography--headersm\">\n                  Команда наших специалистов способна быстро и эффективно решить\n                  любые задачи.\n                </h3>\n                <p class=\"typography typography--text\">\n                  Задачи, связанные с digital-стратегий отдельных продуктов,\n                  запуском и поддержкой highload-сервисов, работой с гибридной\n                  ИТ-инфраструктурой, управлением растущими объемами данных,\n                  аналитикой всех типов. Компания имеет лицензии оператора связи\n                  и оператора персональных данных.\n                </p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </section>\n    );\n}\n\nexport default About;\n"]},"metadata":{},"sourceType":"module"}