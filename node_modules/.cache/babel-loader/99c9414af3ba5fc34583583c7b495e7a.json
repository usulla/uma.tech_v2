ews","map","item","index","hashtag","title","text"],"mappings":";AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAO,aAAP;AACA,OAAOC,MAAP,MAAmB,aAAnB;AACA,OAAO,gCAAP;AACA,OAAO,sCAAP;AAEA,IAAIC,QAAQ,GAAG;AACbC,EAAAA,MAAM,EAAE,KADK;AAEbC,EAAAA,QAAQ,EAAE,IAFG;AAGbC,EAAAA,IAAI,EAAE,IAHO;AAIbC,EAAAA,QAAQ,EAAE,IAJG;AAKbC,EAAAA,KAAK,EAAE,GALM;AAMbC,EAAAA,SAAS,EAAE;AANE,CAAf;;AAQA,MAAMC,IAAI,GAAG,CAAC;AAAEC,EAAAA,QAAF;AAAYC,EAAAA;AAAZ,CAAD,KAA4B;AACvC,MAAI,CAACD,QAAL,EAAe;AACb,WACE;AAAK,MAAA,KAAK,EAAC,kBAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACE;AAAK,MAAA,KAAK,EAAC,qBAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OACGC,QAAQ,CAACC,GAAT,CAAa,CAACC,IAAD,EAAOC,KAAP,KAAiB;AAC7B;AAAK,QAAA,KAAK,iCAA0BA,KAAK,GAAG,CAAlC,sBAAV;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SACE;AAAG,QAAA,KAAK,EAAC,SAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAID,IAAI,CAACE,OAAT,CADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAKF,IAAI,CAACG,KAAV,CAJF,EAKE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAIH,IAAI,CAACI,IAAT,CALF;AAOD,KARA,CADH,CADF,CADF;AAeD;;AACD,SACE,oBAAC,MAAD,oBAAYf,QAAZ;AAAsB,IAAA,KAAK,EAAC,qBAA5B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,MACE;AAAK,IAAA,KAAK,EAAC,+CAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAG,IAAA,KAAK,EAAC,SAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iCADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sXAJF,EAQE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6+DARF,CADF,EAmBE;AAAK,IAAA,KAAK,EAAC,gDAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAG,IAAA,KAAK,EAAC,SAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mDADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,8YAJF,EAQE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2zCARF,CAnBF,EAkCE;AAAK,IAAA,KAAK,EAAC,mDAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAG,IAAA,KAAK,EAAC,SAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iCADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yTAJF,EAOE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ylDAPF,CAlCF,EAkDE;AAAK,IAAA,KAAK,EAAC,sDAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAG,IAAA,KAAK,EAAC,SAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mDADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sNAJF,EAKE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,46BALF,CAlDF,EA6DE;AAAK,IAAA,KAAK,EAAC,oDAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAG,IAAA,KAAK,EAAC,SAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mDADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6PACoD,GADpD,EAEE;AAAG,IAAA,IAAI,EAAC,6BAAR;AAAsC,IAAA,MAAM,EAAC,QAA7C;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBAFF,CAJF,EAUE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,00EAVF,CA7DF,CADF;AAqFD,CAvGD;;AAwGA,eAAeO,IAAf","sourcesContent":["import React from \"react\";\nimport \"./news.scss\";\nimport Slider from \"react-slick\";\nimport \"slick-carousel/slick/slick.css\";\nimport \"slick-carousel/slick/slick-theme.css\";\n\nvar settings = {\n  arrows: false,\n  autoplay: true,\n  dots: true,\n  infinite: true,\n  speed: 600,\n  dotsClass: \"pagination pagination2\"\n};\nconst News = ({ isMobile, dataNews }) => {\n  if (!isMobile) {\n    return (\n      <div class=\"competence__wrap\">\n        <div class=\"competence__content\">\n          {dataNews.map((item, index) => {\n            <div class={`competence__item col-${index + 1} competence__item`}>\n              <p class='hashtag'>\n                <b>{item.hashtag}</b>\n              </p>\n              <h3>{item.title}</h3>\n              <p>{item.text}</p>\n            </div>;\n          })}\n        </div>\n      </div>\n    );\n  }\n  return (\n    <Slider {...settings} class=\"competence__content\">\n      <div class=\"competence__item col-1 competence__item--blue\">\n        <p class=\"hashtag\">\n          <b>#блог</b>\n        </p>\n        <h3>\n          Корпоративная команда Uma.Tech приняла участие в Суперфинале «Гонки\n          Героев»\n        </h3>\n        <p>\n          «Гонка Героев» - популярная командная военно-спортивная игра, где\n          участники преодолевают полосу препятствий, разработанную\n          профессиональными инженерами и опытными инструкторами. Многие из\n          участников прониклись духом командной подготовки, открыли в себе\n          абсолютно новые возможности и теперь не хотят останавливаться на\n          достигнутом. Похоже впереди нас ждет целое спортивное движение\n          {/* :) */}\n        </p>\n      </div>\n      <div class=\"competence__item col-2 competence__item--green\">\n        <p class=\"hashtag\">\n          <b>#новости</b>\n        </p>\n        <h3>\n          Приложение PREMIER вошло в список рекомендуемых сервисов в области\n          цифрового ТВ.\n        </h3>\n        <p>\n          Эксперты Института развития интернета (ИРИ) представили список\n          альтернатив иностранным продуктам, которые могут войти в реестр\n          обязательных отечественных мобильных приложений. Приложение PREMIER\n          вошло в список рекомендуемых сервисов в области цифрового ТВ.\n        </p>\n      </div>\n      <div class=\"competence__item col-3 competence__item--deepblue\">\n        <p class=\"hashtag\">\n          <b>#блог</b>\n        </p>\n        <h3>\n          Uma.Tech стала Генеральным партнёром «Лета Информатики в России»\n        </h3>\n        <p>\n          Лето информатики - двухнедельное научно-образовательное мероприятие,\n          которое включает две крупнейшие международные конференции по Computer\n          Science, проводимые в стране, четыре воркшопа, а также летнюю школу по\n          информатике для студентов. Мероприятие прошло в Академгородке\n          Новосибирского Государственного Университета.\n        </p>\n      </div>\n\n      <div class=\"competence__item col-4 competence__item--green first\">\n        <p class=\"hashtag\">\n          <b>#новости</b>\n        </p>\n        <h3>Повышена производительность серверов</h3>\n        <p>\n          Переход на технологию 25G на уровне видео отдачи позволил платформе\n          Uma.Tech поднять производительность серверов – теперь скорость отдачи\n          выше 100 Гбит/c с одного юнита (больше 4 Тбит/c со стойки).\n        </p>\n      </div>\n      <div class=\"competence__item col-5 competence__item--blue last\">\n        <p class=\"hashtag\">\n          <b>#новости</b>\n        </p>\n        <h3>\n          Запущен Всероссийский студенческий online-конкурс{\" \"}\n          <a href=\"https://challenge.uma.tech/\" target=\"_blank\">\n            Uma.Challenge\n          </a>\n        </h3>\n        <p>\n          В рамках конкурса представлены три основных направления: Highload Dev,\n          Data Science и Machine Learning. Призовой фонд - 250 000 рублей в\n          каждой номинации и специальный стимулирующий приз - MacBook Pro.\n          Зарегистрироваться и выбрать конкурсное направление можно до 25.10.\n          Участники Uma.Challenge получают уникальные шансы для практической\n          демонстрации своих навыков, заявку о себе в среде профессионального\n          сообщества, как о молодых специалистах, доказавших свои способности в\n          решении сложных задач в современных условиях.\n        </p>\n      </div>\n    </Slider>\n  );\n};\nexport default News;\n"]},"metadata":{},"sourceType":"module"}