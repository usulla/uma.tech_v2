A,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iCADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sXAJF,EAQE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6+DARF,CADF,EAmBE;AAAK,IAAA,KAAK,EAAC,gDAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAG,IAAA,KAAK,EAAC,SAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mDADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,8YAJF,EAQE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2zCARF,CAnBF,EAmCE;AAAK,IAAA,KAAK,EAAC,mDAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAG,IAAA,KAAK,EAAC,SAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iCADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yTAJF,EAOE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ylDAPF,CAnCF,EAoDE;AAAK,IAAA,KAAK,EAAC,sDAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAG,IAAA,KAAK,EAAC,SAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mDADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sNAJF,EAKE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,46BALF,CApDF,EAgEE;AAAK,IAAA,KAAK,EAAC,oDAAX;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAG,IAAA,KAAK,EAAC,SAAT;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mDADF,CADF,EAIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6PACoD,GADpD,EAEE;AAAG,IAAA,IAAI,EAAC,6BAAR;AAAsC,IAAA,MAAM,EAAC,QAA7C;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBAFF,CAJF,EAUE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,00EAVF,CAhEF,CADF,CAZF,CADF,CADF;AA0GD,CAnHD;;AAoHA,eAAeD,UAAf","sourcesContent":["import React from \"react\";\nimport \"./competence.scss\";\nimport Slider from \"react-slick\";\nimport \"slick-carousel/slick/slick.css\";\nimport \"slick-carousel/slick/slick-theme.css\";\n\nconst Competence = () => {\n  var settings = {\n    arrows: false,\n    autoplay: true,\n    dots: true,\n    infinite: true,\n    speed: 600,\n    dotsClass: \"pagination pagination2\"\n  };\n  return (\n    <section class=\"section\">\n      <div class=\"competence\">\n        <div class=\"competence__text\">\n          <h2 class=\"typography typography--header\">\n            У нас уникальные компетенции\n          </h2>\n          <p class=\"typography\">\n            В области разработки программного обеспечения, эксплуатации\n            распределенной инфраструктуры, способной работать под высокими\n            нагрузками, аналитики, Big Data, информационной безопасности и\n            управления информацией.\n          </p>\n        </div>\n        <div class=\"competence__wrap\">\n          <Slider {...settings} class=\"competence__content\">\n            <div class=\"competence__item col-1 competence__item--blue\">\n              <p class=\"hashtag\">\n                <b>#блог</b>\n              </p>\n              <h3>\n                Корпоративная команда Uma.Tech приняла участие в Суперфинале\n                «Гонки Героев»\n              </h3>\n              <p>\n                «Гонка Героев» - популярная командная военно-спортивная игра,\n                где участники преодолевают полосу препятствий, разработанную\n                профессиональными инженерами и опытными инструкторами. Многие из\n                участников прониклись духом командной подготовки, открыли в себе\n                абсолютно новые возможности и теперь не хотят останавливаться на\n                достигнутом. Похоже впереди нас ждет целое спортивное движение\n                {/* :) */}\n              </p>\n            </div>\n            <div class=\"competence__item col-2 competence__item--green\">\n              <p class=\"hashtag\">\n                <b>#новости</b>\n              </p>\n              <h3>\n                Приложение PREMIER вошло в список рекомендуемых сервисов в\n                области цифрового ТВ.\n              </h3>\n              <p>\n                Эксперты Института развития интернета (ИРИ) представили список\n                альтернатив иностранным продуктам, которые могут войти в реестр\n                обязательных отечественных мобильных приложений. Приложение\n                PREMIER вошло в список рекомендуемых сервисов в области\n                цифрового ТВ.\n              </p>\n            </div>\n            <div class=\"competence__item col-3 competence__item--deepblue\">\n              <p class=\"hashtag\">\n                <b>#блог</b>\n              </p>\n              <h3>\n                Uma.Tech стала Генеральным партнёром «Лета Информатики в России»\n              </h3>\n              <p>\n                Лето информатики - двухнедельное научно-образовательное\n                мероприятие, которое включает две крупнейшие международные\n                конференции по Computer Science, проводимые в стране, четыре\n                воркшопа, а также летнюю школу по информатике для студентов.\n                Мероприятие прошло в Академгородке Новосибирского\n                Государственного Университета.\n              </p>\n            </div>\n\n            <div class=\"competence__item col-4 competence__item--green first\">\n              <p class=\"hashtag\">\n                <b>#новости</b>\n              </p>\n              <h3>Повышена производительность серверов</h3>\n              <p>\n                Переход на технологию 25G на уровне видео отдачи позволил\n                платформе Uma.Tech поднять производительность серверов – теперь\n                скорость отдачи выше 100 Гбит/c с одного юнита (больше 4 Тбит/c\n                со стойки).\n              </p>\n            </div>\n            <div class=\"competence__item col-5 competence__item--blue last\">\n              <p class=\"hashtag\">\n                <b>#новости</b>\n              </p>\n              <h3>\n                Запущен Всероссийский студенческий online-конкурс{\" \"}\n                <a href=\"https://challenge.uma.tech/\" target=\"_blank\">\n                  Uma.Challenge\n                </a>\n              </h3>\n              <p>\n                В рамках конкурса представлены три основных направления:\n                Highload Dev, Data Science и Machine Learning. Призовой фонд -\n                250 000 рублей в каждой номинации и специальный стимулирующий\n                приз - MacBook Pro. Зарегистрироваться и выбрать конкурсное\n                направление можно до 25.10. Участники Uma.Challenge получают\n                уникальные шансы для практической демонстрации своих навыков,\n                заявку о себе в среде профессионального сообщества, как о\n                молодых специалистах, доказавших свои способности в решении\n                сложных задач в современных условиях.\n              </p>\n            </div>\n          </Slider>\n        </div>\n      </div>\n    </section>\n  );\n};\nexport default Competence;\n"]},"metadata":{},"sourceType":"module"}