3\\u043E \\u0440\\u043E\\u0441\\u0442\\u0430 \\u0438 \\u043B\\u0438\\u0447\\u043D\\u043E\\u0441\\u0442\\u043D\\u043E\\u0433\\u043E \\u0440\\u0430\\u0437\\u0432\\u0438\\u0442\\u0438\\u044F \\u043A\\u0430\\u0436\\u0434\\u043E\\u0433\\u043E \\u0441\\u043E\\u0442\\u0440\\u0443\\u0434\\u043D\\u0438\\u043A\\u0430 \\u0432 \\u043D\\u0430\\u0448\\u0435\\u0439 \\u043A\\u043E\\u043C\\u0430\\u043D\\u0434\\u0435, \\u0447\\u0442\\u043E \\u043F\\u043E\\u0437\\u0432\\u043E\\u043B\\u044F\\u0435\\u0442 \\u043D\\u0430\\u043C \\u043E\\u0442\\u0432\\u0435\\u0447\\u0430\\u0442\\u044C \\u0432\\u044B\\u0437\\u043E\\u0432\\u0430\\u043C \\u043B\\u044E\\u0431\\u043E\\u0433\\u043E \\u043C\\u0430\\u0441\\u0448\\u0442\\u0430\\u0431\\u0430 \\u0438 \\u0443\\u0440\\u043E\\u0432\\u043D\\u044F \\u0441\\u043B\\u043E\\u0436\\u043D\\u043E\\u0441\\u0442\\u0438 \\u043A\\u0430\\u043A \\u0441\\u0435\\u0439\\u0447\\u0430\\u0441, \\u0442\\u0430\\u043A \\u0438 \\u0432 \\u043F\\u0435\\u0440\\u0441\\u043F\\u0435\\u043A\\u0442\\u0438\\u0432\\u0435.\")))));\n};\n\nexport default AboutUs;","map":{"version":3,"sources":["/Users/msukhareva/Documents/projects/uma.tech.v2/src/pages/AboutUs/AboutUs.jsx"],"names":["React","AboutUs","isMobile","items","handleClick","e","self","currentTarget"],"mappings":";AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAO,iBAAP;;;AAGA,MAAMC,OAAO,GAAG,CAAC;AAAEC,EAAAA,QAAF;AAAYC,EAAAA;AAAZ,CAAD,KAAyB;AACvC,WAASC,WAAT,CAAqBC,CAArB,EAAwB;AACtB,UAAMC,IAAI,GAAGD,CAAC,CAACE,aAAf;AACD;;AACD,SACE;AAAK,IAAA,SAAS,EAAC,qBAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,SAAS,EAAC,mBAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAI,IAAA,SAAS,EAAC,yCAAd;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,UADF,8BADF,EAIE;AAAK,IAAA,SAAS,EAAC,iBAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAEE;AAAK,IAAA,SAAS,EAAC,2CAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gMAEE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAFF,0JAGE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,IAHF,uKADF,EAME;AAAG,IAAA,SAAS,EAAC,aAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mnFANF,CAFF,EAqBE;AAAK,IAAA,SAAS,EAAC,sDAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qEADF,EAEE;AAAK,IAAA,SAAS,EAAC,mBAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,SAAS,EAAC,QAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,SAAS,EAAC,OAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sOADF,EAIE;AAAK,IAAA,SAAS,EAAC,aAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,utDAJF,CADF,EAcE;AAAK,IAAA,SAAS,EAAC,QAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACG,GADH,EAEE;AAAK,IAAA,SAAS,EAAC,OAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gZAFF,EAME;AAAK,IAAA,SAAS,EAAC,aAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,m/BANF,CAdF,EA2BE;AAAK,IAAA,SAAS,EAAC,QAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAK,IAAA,SAAS,EAAC,OAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uMADF,EAIE;AAAK,IAAA,SAAS,EAAC,aAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,+2CAJF,CA3BF,CAFF,CArBF,EAiEE;AAAK,IAAA,SAAS,EAAC,2CAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,wDADF,EAEE;AAAG,IAAA,SAAS,EAAC,aAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2vDAFF,EAUE;AAAK,IAAA,SAAS,EAAC,MAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6vBAVF,EAeE;AAAK,IAAA,SAAS,EAAC,MAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6vBAfF,EAoBE;AAAK,IAAA,SAAS,EAAC,MAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uJADF,m6DApBF,CAjEF,CAJF,CADF,CADF;AAyGD,CA7GD;;AA8GA,eAAeN,OAAf","sourcesContent":["import React from \"react\";\nimport \"./about_us.scss\";\nimport arrow_light from \"../../images/arrow_light.svg\";\n\nconst AboutUs = ({ isMobile, items }) => {\n  function handleClick(e) {\n    const self = e.currentTarget;\n  }\n  return (\n    <div className=\"page about-us__wrap\">\n      <div className=\"about-us__content\">\n        <h1 className=\"typography typography--header container\">\n          <span>| </span>О нас\n        </h1>\n        <div className=\"about-us__items\">\n          {/* 1 */}\n          <div className=\"about-us__item container about-us__item-1\">\n            <h2>\n              Мы – высокотехнологичная компания\n              <br /> с многолетним опытом работы\n              <br /> на рынке ИТ и телекоммуникаций.\n            </h2>\n            <p className=\"description\">\n              Наша история началась несколько лет назад с небольшой команды\n              разработчиков и инженеров. Сегодня у нас большое сообщество\n              единомышленников и профессионалов, котоые создают самые передовые\n              технологии для медиа-индустрии и решают сложнейшие задачи\n              видеоаналитики, оптимизации и анализа Big Data. Мы всегда\n              стремимся вперед, предвосхищая развитие индустрии и помогая людям\n              получать максимальное удовольствие от вселенной развлекательного\n              контента. Uma.Tech - оператор связи, имеет лицензию оператора\n              персональных данных.\n            </p>\n          </div>\n          {/* 2 */}\n          <div className=\"about-us__item about-us__item-2 about-us__item--blue\">\n            <h2>Наша миссия</h2>\n            <div className=\"branchs container\">\n              <div className=\"branch\">\n                <div className=\"title\">\n                  Мы строим цифровую экономику новой России,\n                </div>\n                <div className=\"description\">\n                  создавая решения и поставляя сервисы для заказчиков и структур\n                  из разных сфер деятельности. Среди наших реальных и\n                  потенциальных клиентов – государственные предприятия и частные\n                  компании, разного размера и из различной специализации,\n                  применяющие современные технологии и реализующие «цифровую\n                  трансформацию» своей деятельности.\n                </div>\n              </div>\n              <div className=\"branch\">\n                {\" \"}\n                <div className=\"title\">\n                  Наша инфраструктура охватывает все экономически активные\n                  регионы страны:\n                </div>\n                <div className=\"description\">\n                  дата-центры позволяют работать с огромными объемами данных,\n                  коммуникационные каналы – получать скоростной доступ к\n                  современному цифровому контенту, включая самые тяжелые его\n                  разновидности.\n                </div>\n              </div>\n              <div className=\"branch\">\n                <div className=\"title\">\n                  Мы умеем создавать цифровые решения,\n                </div>\n                <div className=\"description\">\n                  способные развивать бизнес-процессы наших клиентов, поднимать\n                  на новый уровень эффективность основной их деятельности. Наша\n                  квалификация позволяет клиентам радикально изменять\n                  производство, внедрять инновационные технологии в управление,\n                  маркетинг и продажи.\n                </div>\n              </div>\n            </div>\n          </div>\n          {/* 3 */}\n          <div className=\"about-us__item container about-us__item-3\">\n            <h2>Ценности</h2>\n            <p className=\"description\">\n              Наши ценности: свобода творчества в развитии цифрового мира,\n              самореализация в создании современнейших решений и разработка\n              digital-инструментов для расцвета национальной экономики. Мы имеем\n              практический опыт разработки сервисов разных масштабов – от\n              работающих на маленькие стартапы до обеспечивающих работу компаний\n              федерального масштаба.\n            </p>\n            <div className=\"item\">\n              Наша уникальность в умении проектировать, разрабатывать, внедрять,\n              поддерживать и развивать решения, работающие под экстремальными\n              нагрузками.\n            </div>\n            <div className=\"item\">\n              Мы можем принимать, хранить и обрабатывать огромные объемы данных,\n              превращая «цифровую руду» в информацию, значимую для государства и\n              для бизнеса.\n            </div>\n            <div className=\"item\">\n              <span>Наш основной капитал – люди.</span> Команда наших\n              сотрудников способна очень быстро и крайне качественно создавать,\n              внедрять и развивать сервисы любого уровня сложности. Наша\n              стратегия состоит в обеспечении непрерывного профессионального\n              роста и личностного развития каждого сотрудника в нашей команде,\n              что позволяет нам отвечать вызовам любого масштаба и уровня\n              сложности как сейчас, так и в перспективе.\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\nexport default AboutUs;\n"]},"metadata":{},"sourceType":"module"}